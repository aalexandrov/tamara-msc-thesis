<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--************************************************************************
    * Data Generators
    *************************************************************************-->

    <bean id="datagen.spark.int" class="eu.stratosphere.peel.extensions.spark.beans.job.SparkJob">
        <constructor-arg name="runner" ref="spark-1.3.1"/>
        <constructor-arg name="command">
            <value>--class eu.stratosphere.procrustes.datagen.spark.SparkIntGenerator ${app.path.datagens}/tamaras-msc-thesis-spark-datagens-1.0-SNAPSHOT.jar ${system.spark.config.defaults.spark.master} ${system.default.config.parallelism.total} ${datagen.tuples.per.task} ${datagen.distribution} ${system.hadoop-2.path.input}/operatorstats/${datagen.path}</value>
        </constructor-arg>
    </bean>

    <!--************************************************************************
    * Data Sets -> Number of nodes and distribution determines the sets
    *************************************************************************-->

    <bean id="dataset.int" class="eu.stratosphere.peel.core.beans.data.GeneratedDataSet">
        <constructor-arg name="src" ref="datagen.spark.int"/>
        <constructor-arg name="dst" value="${system.hadoop-2.path.input}/operatorstats/${datagen.path}"/>
        <constructor-arg name="fs" ref="hdfs-2.4.1"/>
    </bean>

    <!--************************************************************************
    * Experiments
    *************************************************************************-->

    <!-- Experiment output -->
    <bean id="experiment.output.hdfs.operator.stats" class="eu.stratosphere.peel.core.beans.data.ExperimentOutput">
        <constructor-arg name="path" value="${system.hadoop-2.path.output}/operatorstats/output"/>
        <constructor-arg name="fs" ref="hdfs-2.4.1"/>
    </bean>

    <bean id="operator.stats.processing" parent="experiment.flink-0.9-FLINK-1297" abstract="true">
        <constructor-arg name="command">
            <value>-v -c eu.stratosphere.procrustes.experiments.operator.stats.FlatMap ${app.path.jobs}/tamaras-msc-thesis-flink-jobs-1.0-SNAPSHOT.jar ${system.hadoop-2.path.input}/operatorstats/${datagen.path} ${operator.stats.algorithm}</value>
        </constructor-arg>
        <constructor-arg name="input">
            <set value-type="eu.stratosphere.peel.core.beans.data.GeneratedDataSet">
                <ref bean="dataset.int"/>
            </set>
        </constructor-arg>
        <constructor-arg name="output">
            <ref bean="experiment.output.hdfs.operator.stats"/>
        </constructor-arg>
    </bean>

    <bean id="heavy.hitters.experiment" parent="experiment.flink-0.9-FLINK-1297" abstract="true">
        <constructor-arg name="command">
            <value>-v -c eu.stratosphere.procrustes.experiments.operator.stats.HeavyHitterNoAccumulator ${app.path.jobs}/tamaras-msc-thesis-flink-jobs-1.0-SNAPSHOT.jar ${system.hadoop-2.path.input}/operatorstats/${datagen.path} ${operator.stats.cardinality}</value>
        </constructor-arg>
        <constructor-arg name="input">
            <set value-type="eu.stratosphere.peel.core.beans.data.GeneratedDataSet">
                <ref bean="dataset.int"/>
            </set>
        </constructor-arg>
        <constructor-arg name="output">
            <ref bean="experiment.output.hdfs.operator.stats"/>
        </constructor-arg>
    </bean>

    <!--************************************************************************
    * Experiments
    *************************************************************************-->

    <!-- processing scale-out (on 5, 10, and 20 nodes) with 200 million elements per node -->
    <bean id="processing.scale-out.10M" class="eu.stratosphere.peel.core.beans.experiment.ExperimentSuite">
        <constructor-arg name="experiments">
            <!-- no stats collection (none.XX) -->
            <list value-type="eu.stratosphere.peel.core.beans.experiment.Experiment">
                <bean parent="operator.stats.processing">
                    <constructor-arg name="name" value="none.20"/>
                    <constructor-arg name="config">
                        <value>
                            # common system parameters
                            system.default.config.parallelism.total = 160
                            system.default.config.slaves = ${env.hosts.slaves.only20}
                            # experiment parameters
                            operator.stats.algorithm = none
                            # datagen parameters
                            datagen.path = 4B
                            datagen.tuples.per.task = 25000000
                            datagen.distribution = "Uniform[100000]"
                        </value>
                    </constructor-arg>
                </bean>
                <bean parent="operator.stats.processing">
                    <constructor-arg name="name" value="none.10"/>
                    <constructor-arg name="config">
                        <value>
                            # common system parameters
                            system.default.config.parallelism.total = 80
                            system.default.config.slaves = ${env.hosts.slaves.only10}
                            # experiment parameters
                            operator.stats.algorithm = none
                            # datagen parameters
                            datagen.path = 2B
                            datagen.tuples.per.task = 25000000
                            datagen.distribution = "Uniform[100000]"
                        </value>
                    </constructor-arg>
                </bean>
                <bean parent="operator.stats.processing">
                    <constructor-arg name="name" value="none.05"/>
                    <constructor-arg name="config">
                        <value>
                            # common system parameters
                            system.default.config.parallelism.total = 40
                            system.default.config.slaves = ${env.hosts.slaves.only05}
                            # experiment parameters
                            operator.stats.algorithm = none
                            # datagen parameters
                            datagen.path = 1B
                            datagen.tuples.per.task = 25000000
                            datagen.distribution = "Uniform[100000]"
                        </value>
                    </constructor-arg>
                </bean>
            </list>
        </constructor-arg>
    </bean>

